{
  "name": "scottspence.com",
  "private": true,
  "description": "a digital garden",
  "version": "0.1.0",
  "author": {
    "name": "Scott Spence",
    "email": "yo@scottspence.dev",
    "url": "https://scottspence.com"
  },
  "license": "MIT",
  "scripts": {
    "dev": "gatsby develop -p 5421 -o",
    "build": "gatsby build && yarn wellknown && yarn robots",
    "serve": "gatsby serve -p 5400 -o",
    "format": "pretty-quick",
    "wellknown": "cp -r .well-known/ public/",
    "robots": "cp robots.txt public/",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:e2e:dev": "$(yarn bin)/cypress open",
    "test:e2e": "$(yarn bin)/cypress"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@mdx-js/mdx": "^2.0.0-next.8",
    "@mdx-js/react": "^2.0.0-next.8",
    "fontsource-hepta-slab": "^4.0.0",
    "fontsource-work-sans": "^4.0.0",
    "fuse.js": "^6.4.3",
    "gatsby": "^2.29.2",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-brotli": "^2.0.0",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-mdx": "^1.9.0-next.0",
    "gatsby-plugin-mdx-embed": "^0.0.17",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-sharp": "^2.11.2",
    "gatsby-plugin-ts-config": "^1.1.5",
    "gatsby-remark-autolink-headers": "^2.8.0",
    "gatsby-remark-copy-linked-files": "^2.7.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^3.8.1",
    "gatsby-remark-smartypants": "^2.7.0",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-transformer-sharp": "^2.6.0",
    "mdx-embed": "^0.0.17",
    "mdx-utils": "^0.2.0",
    "prism-react-renderer": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.16.0",
    "react-icons": "^4.1.0",
    "react-live": "^2.2.3",
    "react-seo-component": "^2.0.2",
    "theme-ui": "^0.3.5",
    "victormono": "^1.4.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react-highlight-words": "^0.16.1",
    "@types/theme-ui": "^0.3.7",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.9.1",
    "cypress": "^6.2.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "typescript": "^4.1.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
